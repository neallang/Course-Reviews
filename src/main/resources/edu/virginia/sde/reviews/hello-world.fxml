   <children>
      <VBox layoutX="79.0" layoutY="67.0" prefHeight="200.0" prefWidth="21.0" />
      <VBox layoutX="30.0" layoutY="35.0" prefHeight="200.0" prefWidth="203.0">
         <children>
            <Label prefHeight="17.0" prefWidth="146.0" text="Press the button" />
            <Label fx:id="messageLabel" />
            <Button fx:id="not_button" mnemonicParsing="false" onAction="#handleButton" text="Not a button" textAlignment="RIGHT" textFill="#c64a4a" />
            <Button mnemonicParsing="false" onAction="#handleButton" text="Button" />
         </children>
      </VBox>
   </children>
</AnchorPane>
